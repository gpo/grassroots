        "required": ["rowsToSkip", "rowsToTake"],
        "type": "object"
      },
      "PaginatedOutDTO": {
        "properties": {
          "rowsSkipped": {
            "minimum": 0,
            "type": "number"
          },
          "rowsTotal": {
            "minimum": 0,
            "type": "number"
          }
        },
        "required": ["rowsSkipped", "rowsTotal"],

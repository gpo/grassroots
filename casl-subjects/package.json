{
  "name": "casl-subjects",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "exports": {
    "./*": {
      "require": "./dist-gen/*.js",
      "import": "./dist-gen/*.js",
      "types": "./dist-gen/*.d.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "gen-only": "tsc && node --enable-source-maps dist/GenerateSubjects.js",
    "build-only": "tsc --emitDeclarationOnly -p tsconfig-generated.json && swc --strip-leading-paths gen --out-dir dist-gen",
    "build": "pnpm run gen-only && pnpm run build-only",
    "clean": "rm -rf dist dist-gen && rm -f tsconfig.tsbuildinfo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.3.0",
    "ts-morph": "^26.0.0",
    "typescript": "^5.9.2"
  }
}

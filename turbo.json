{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "test": {
      "dependsOn": ["^build", "^tsc"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build", "^tsc"],
      "outputs": [
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist-build/**/*.js",
        "dist-build/**/*.js.map"
      ]
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": [
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map",
        "dist-build/**/*.d.ts",
        "dist-build/**/*.d.ts.map"
      ]
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": ["eslint_rules#build"]
    }
  }
}

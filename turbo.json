{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*.js", "dist/**/*.js.map"]
    },
    "type-check": {
      "dependsOn": ["^type-check"]
    },
    "test": {
      "dependsOn": ["grassroots-backend#gen-files-only"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "//#lint": {
      "dependsOn": ["eslint_rules#build"],
      "cache": false
    }
  }
}

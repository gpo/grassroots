{
  "name": "grassroots-shared-net",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "swc src --strip-leading-paths --out-dir dist --config-file=../.swcrc",
    "build-watch": "swc src --strip-leading-paths --out-dir dist --config-file=../.swcrc --watch",
    "tsc": "tsc",
    "tsc-watch": "nice -n 10 tsc --watch",
    "clean": "rm -rf .turbo && rm -rf dist && rm -f *.tsbuildinfo && rm -rf /app/.turbo"
  },
  "keywords": [],
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.11.0",
  "type": "module",
  "composite": true,
  "exports": {
    "./*": {
      "require": "./src/**/*.ts",
      "import": "./src/**/*.ts",
      "types": "./src/**/*.ts"
    }
  },
  "dependencies": {
    "grassroots-shared": "workspace:*",
    "openapi-fetch": "^0.13.8",
    "openapi-paths": "workspace:*"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
